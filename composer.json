{
  "name": "elasticsearch/elasticsearch-legacy",
  "description": "PHP Client for Elasticsearch Legacy",
  "keywords": ["search","client", "elasticsearch"],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Zachary Tong"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/log": "~1.0|~2.0",
    "guzzlehttp/ringphp" : "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5",
    "mockery/mockery": "0.9.4",
    "symfony/yaml": "~2.4.3",
    "twig/twig": "1.*",
    "cpliakas/git-wrapper": "~1.0"
  },
  "suggest": {
    "ext-curl": "*",
    "monolog/monolog": "Allows for client-level logging and tracing"
  },
  "autoload": {
    "psr-4": {
      "ElasticsearchLegacy\\": "src/ElasticsearchLegacy/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElasticsearchLegacy\\Tests\\":      "tests/ElasticsearchLegacy/Tests/"
    }
  }
}
